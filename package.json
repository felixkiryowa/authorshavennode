{
  "name": "auhorshavennode",
  "version": "1.0.0",
  "description": "Creating an API in node js",
  "main": "index.js",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "npm run undomigration && npm run migration && babel-node index.js",
    "migration": "babel-node node_modules/.bin/sequelize db:migrate",
    "undomigration": "babel-node node_modules/.bin/sequelize db:migrate:undo:all",
    "start:dev": "nodemon ./node_modules/.bin/babel-node index.js",
    "test": "export NODE_ENV=test && npm run undomigration && npm run migration && nyc --reporter=html --reporter=text mocha --timeout 90000 ./test --exit --require @babel/register",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage"
  },
  "author": "Francis Kiryowa",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai-spies": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "express-validator": "^6.1.1",
    "gravatar": "^1.8.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "morgan": "^1.9.1",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-facebook-token": "^3.3.0",
    "passport-google-oauth": "^2.0.0",
    "passport-google-plus-token": "^2.1.0",
    "passport-jwt": "^4.0.0",
    "passport-twitter": "^1.0.4",
    "passport-twitter-token": "^1.3.0",
    "path": "^0.12.7",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "prettier": "^1.18.2",
    "sequelize": "^5.16.0",
    "sequelize-cli": "^5.5.0",
    "sinon": "^7.4.2",
    "validator": "^11.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.3.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "nyc": "^14.1.1"
  }
}
